{
  "name": "gatsby-source-greenhouse-job-board",
  "version": "2.0.0",
  "description": "Gatsby source plugin for pulling offices, departments, and jobs into Gatsby from the Greenhouse Job Board API. It creates links between offices, departments, and jobs so they can be queried in Gatsby using GraphQL.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinbarnett/gatsby-source-greenhouse-job-board.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "greenhouse"
  ],
  "author": "Kevin Barnett",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.20.13",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "babel-preset-gatsby-package": "^3.15.0",
    "cross-env": "^7.0.3"
  },
  "peerDependencies": {
    "gatsby": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  }
}
